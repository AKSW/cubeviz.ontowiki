<> foaf:primaryTopic :cubeviz .
:cubeviz a doap:Project ;

  doap:release :v1-0 ;
  doap:description "" ;
  doap:name "CubeViz" ;
  rdfs:label "CubeViz" ;

  owconfig:authorLabel "" ;
  owconfig:enabled "true"^^xsd:boolean ;
